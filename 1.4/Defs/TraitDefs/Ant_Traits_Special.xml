<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- Queen -->
  <TraitDef>
    <defName>Ant_HiveQueenTrait</defName>
    <commonality>0</commonality>
    <commonalityFemale>1</commonalityFemale>
    <disabledWorkTags>Violent</disabledWorkTags>
    <disabledWorkTypes>
      <li>Firefighter</li>
      <li>BasicWorker</li>
      <li>Handling</li>
      <li>Cooking</li>
      <li>Hunting</li>
      <li>Construction</li>
      <li>Growing</li>
      <li>Mining</li>
      <li>PlantCutting</li>
      <li>Smithing</li>
      <li>Tailoring</li>
      <li>Art</li>
      <li>Crafting</li>
      <li>Hauling</li>
      <li>Cleaning</li>
      <li>Research</li>
    </disabledWorkTypes>
    <degreeDatas>
      <li>
        <label>antinium hive queen</label>
        <description>{PAWN_nameDef} was born to rule.\n\nOnly female Antinium have this trait. Queens are special, some of the special functions will not work with other pawns.</description>
        <marketValueFactorOffset>1.50</marketValueFactorOffset>
        <statOffsets>
          <MoveSpeed>-0.2</MoveSpeed>
          <WorkSpeedGlobal>-0.30</WorkSpeedGlobal>
        </statOffsets>
      </li>
    </degreeDatas>
    <conflictingTraits>
      <!--<li>Ant_Farmer</li>-->
      <li>Ant_Digger</li>
      <li>Ant_Empath</li>
      <li>Ant_TunnelGuardian</li>
      <!--<li>Ant_Artisan</li>-->
      <!--<li>Ant_Architect</li>-->
      <li>Ant_Ranger</li>
      <li>Ant_Warden</li>
      <li>Ant_GeneResearcher</li>
      <!--<li>Ant_Patrol</li>-->
      <li>Ant_Caretaker</li>
    </conflictingTraits>
  </TraitDef>
  <!--  Antinium -->
  <TraitDef>
    <!-- Ranger -->
    <defName>Ant_Ranger</defName>
    <disabledWorkTypes>
      <li>Doctor</li>
      <li>Smithing</li>
      <li>Tailoring</li>
      <li>Crafting</li>
    </disabledWorkTypes>
    <degreeDatas>
      <li>
        <label>antinium ranger</label>
        <description>{PAWN_nameDef} was designed to patroll the outskirts of [PAWN_possessive] territory for intruders.</description>
        <marketValueFactorOffset>0.40</marketValueFactorOffset>
        <statOffsets>
          <MiningSpeed>.10</MiningSpeed>
          <FoodPoisonChance>-.10</FoodPoisonChance>
          <ShootingAccuracyPawn>3</ShootingAccuracyPawn>
        </statOffsets>
      </li>
    </degreeDatas>
    <conflictingTraits>
      <li>Ant_HiveQueenTrait</li>
      <!--<li>Ant_Farmer</li>-->
      <li>Ant_Digger</li>
      <li>Ant_Empath</li>
      <li>Ant_TunnelGuardian</li>
      <!--<li>Ant_Artisan</li>-->
      <!--<li>Ant_Architect</li>-->
      <li>Ant_Warden</li>
      <li>Ant_GeneResearcher</li>
      <!--<li>Ant_Patrol</li>-->
      <li>Ant_Caretaker</li>
    </conflictingTraits>
  </TraitDef>
