    <!-- Nurturing: Caretaker -->
    <defName>Ant_Caretaker</defName>
    <degreeDatas>
      <li>
        <label>antinium caretaker</label>
        <description>{PAWN_nameDef} has a nurturing nature.</description>
        <marketValueFactorOffset>0.40</marketValueFactorOffset>
        <statOffsets>
          <TradePriceImprovement>.10</TradePriceImprovement>
          <PlantHarvestYield>.15</PlantHarvestYield>
