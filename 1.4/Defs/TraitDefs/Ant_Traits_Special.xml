      <li>Ant_GeneResearcher</li>
      <!--<li>Ant_Patrol</li>-->
      <li>Ant_Caretaker</li>
    </conflictingTraits>
  </TraitDef>
  <TraitDef>
    <!-- Genetic Researcher -->
    <defName>Ant_GeneResearcher</defName>
    <disabledWorkTypes>
      <li>Mining</li>
      <li>Warden</li>
      <li>Hauling</li>
      <li>Cleaning</li>
    </disabledWorkTypes>
    <degreeDatas>
      <li>
        <label>antinium geneticist</label>
        <description>{PAWN_nameDef} conducts genetic research for the hive, usually by taking creatures apart to learn how they work.</description>
        <marketValueFactorOffset>0.40</marketValueFactorOffset>
        <statOffsets>
          <ResearchSpeed>.05</ResearchSpeed>
          <MedicalSurgerySuccessChance>.10</MedicalSurgerySuccessChance>
          <DrugSynthesisSpeed>.10</DrugSynthesisSpeed>
          <ButcheryFleshSpeed>.10</ButcheryFleshSpeed>
          <ButcheryFleshEfficiency>.10</ButcheryFleshEfficiency>
        </statOffsets>
      </li>
    </degreeDatas>
    <conflictingTraits>
      <li>Ant_HiveQueenTrait</li>
      <!--<li>Ant_Farmer</li>-->
      <li>Ant_Digger</li>
      <li>Ant_Empath</li>
      <li>Ant_TunnelGuardian</li>
